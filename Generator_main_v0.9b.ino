        amplitude[optTop] = amplitude[optTop] + inc*0.05f;
        if(amplitude[optTop] > 1.6f) amplitude[optTop] = 0.0f;
        if(amplitude[optTop] < -0.01f) amplitude[optTop] = 1.6f;
        break;
      };
      case 3: {
        phase[optTop] = phase[optTop] + inc*10;
        if(phase[optTop] > NUM_SAMPLES) phase[optTop] = 0;
        if(phase[optTop] < 0) phase[optTop] = NUM_SAMPLES;
        break;
      };
    }
  }
}

void drawCChannel(){
  int N = mcm(ceil(NUM_SAMPLES/frequency[0]), ceil(NUM_SAMPLES/frequency[1]));
  int V[N];
  int j = 0;
  int k = 0;
  int m = 0;
  
  for(int i = 0; i < N; i++) {
    V[i] = amplitude[0]*WaveFormTable[wave_type[0]][j] + amplitude[1]*WaveFormTable[wave_type[1]][k + phase[1]];
    
    j += frequency[0];
    k += frequency[1];
    
    if (j >= NUM_SAMPLES) j = 0; 
    if (k >= NUM_SAMPLES - phase[1]) k = -phase[1];
  }  

  for(int n = 0; n < 128; n++){
    display.setPixel(n, 63 - 47*V[(int)(n*N/128)]/255);
    //N has to change in case of overmix
    
    //if(n % 18 == 0) display.drawLine(n, 40, n+9, 40);    
    //display.drawString(7, 35, "0V");
  }
}

